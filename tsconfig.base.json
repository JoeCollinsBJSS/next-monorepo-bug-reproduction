{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "baseUrl": "./packages",
    "strict": true,
    "paths": {
      "@caas-ce-api/*": ["packages/*"]
    }
  }
}
